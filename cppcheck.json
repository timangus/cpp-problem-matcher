{
    "problemMatcher":
    [
        {
            "owner": "cpp-problem-matcher",
            "pattern":
            [
                {
                    "regexp": "^((\\s*\\-\\>\\s*)?\\[([^:]+):(\\d+)\\])+:\\s*\\(([^\\)]*)\\)\\s*(.*)$",
                    "file": 3,
                    "line": 4,
                    "severity": 5,
                    "message": 6
                }
            ]
        }
    ]
}
