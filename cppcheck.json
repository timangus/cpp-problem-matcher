{
    "problemMatcher":
    [
        {
            "owner": "cpp-problem-matcher-cppcheck",
            "severity": "warning",
            "pattern":
            [
                {
                    "regexp": "^\\s*<error(\\s+(id=\"(?<code>[^\"]+)\"|severity=\"(?<severity>[^\"]+)\"|verbose=\"(?<message>[^\"]+)\"|[^=]+=\"[^\"]+\"))+>\\s*<location(\\s+(file=\"(?<file>[^\"]+)\"| line=\"(?<line>[^\"]+)\"|column=\"(?<column>[^\"]+)\"|[^=]+=\"[^\"]+\"))+\\/>\\s*<\\/error>$",
                    "file": 3,
                    "line": 4,
                    "severity": 6,
                    "message": 8,
                    "code": 5
                }
            ]
        }
    ]
}
