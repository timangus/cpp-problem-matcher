{
    "problemMatcher":
    [
        {
            "owner": "cpp-problem-matcher-cppcheck",
            "severity": "warning",
            "pattern":
            [
                {
                    "regexp": "^((\\s*\\-\\>\\s*)?\\[([^:]+):(\\d+)\\])+:\\s*\\(((warning|error)|([^\\)]+))\\)\\s*(.*)$",
                    "file": 3,
                    "line": 4,
                    "severity": 6,
                    "message": 8,
                    "code": 5
                }
            ]
        }
    ]
}
